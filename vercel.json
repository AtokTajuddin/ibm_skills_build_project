{
  "framework": "create-react-app",
  "buildCommand": "cd frontend && npm install --force && npm run build",
  "outputDirectory": "frontend/build",
  "regions": ["fra1"],
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index.js" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
